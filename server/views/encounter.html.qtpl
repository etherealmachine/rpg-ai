{% import (
  "github.com/etherealmachine/rpg.ai/server/models"
) %}

{% code
type EncounterPage struct {
  *BasePage
  Encounter models.Encounter
  Tilemap models.Tilemap
  Character *models.Character
}
%}

{% func (p *EncounterPage) Content() %}
  <div style="flex: 1" class="Encounter" data-props="{%q JSON(map[string]interface{}{
    "Encounter": p.Encounter,
    "Tilemap": p.Tilemap,
    "Character": p.Character,
    })
  %}"></div>
{% endfunc %}